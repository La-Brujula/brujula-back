    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.16",
    "@types/nodemailer": "^6.4.14",
    "@types/nodemailer-express-handlebars": "^4.0.5",
    "@types/validator": "^13.11.8",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3"
