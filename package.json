  "main": "main.ts",
  "license": "MIT",
  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "start": "node dist/main.js",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
