    "express-fileupload": "^1.4.3",
    "express-handlebars": "^7.1.2",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "husky": "^9.0.11",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.9.9",
    "nodemailer-express-handlebars": "^6.1.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "sequelize": "^6.35.2",
    "sequelize-typescript": "^2.1.6",
